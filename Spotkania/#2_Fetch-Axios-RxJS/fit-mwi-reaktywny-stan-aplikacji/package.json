  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "premock:json": "npm install -g json-server",
    "mock:json": "json-server --watch mock-data.json",
    "premock:js": "npm install -g json-server",
    "mock:js": "json-server --watch mock-data.js",
    "build": "ng build --prod && workbox injectManifest",
    "test": "ng test --browsers Chrome --watch true",
    "test:console": "ng test --watch=false --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "pre-commit": [
    "lint",
    "test:console"
  ],
